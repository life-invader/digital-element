{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "warn",
        "semi": "warn",
        "eol-last": "off",
        "no-unused-vars": "warn",
        "arrow-body-style": "warn"
    },
    "ignorePatterns": [
        "webpack.config.js"
    ]
}